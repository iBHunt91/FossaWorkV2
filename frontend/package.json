{
  "name": "fossawork-v2-frontend",
  "version": "2.0.0",
  "description": "Modern React frontend for FossaWork V2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite",
    "dev:win": "set NODE_OPTIONS=--max-old-space-size=8192 && vite",
    "dev:simple": "vite",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite build",
    "build:win": "set NODE_OPTIONS=--max-old-space-size=8192 && vite build",
    "preview": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vite preview",
    "test": "cross-env NODE_OPTIONS=--max-old-space-size=8192 vitest",
    "lint": "cross-env NODE_OPTIONS=--max-old-space-size=8192 eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-query": "^4.35.0",
    "@types/leaflet": "^1.9.18",
    "axios": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-router-dom": "^6.15.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "postcss": "^8.5.4",
    "shadcn": "^2.6.0-canary.3",
    "tailwindcss": "^3.4.16",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.0"
  }
